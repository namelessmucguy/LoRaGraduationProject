<!--pages/capacitys_data/capacitys_data.wxml-->
<!--地图容器-->
<map
id="myMap"
style="width: 100%; height: 300px;"
scale='16'
latitude="{{latitude}}" 
longitude="{{longitude}}"
polyline="{{polyline}}"
markers="{{markers}}"
show-location="true"
>
</map>
<view class="text">剩余容量</view>
<view class="progress-item" >
    <i-progress percent="{{percent}}" status="active"></i-progress>
</view>
<view class="text">坐标位置</view>
<view class="text">{{location}}</view>
<view class="text">设备状态</view>
<view class="text">{{sta}}</view>
<view class="text">工作频率：433MHZ</view>